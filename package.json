{
  "name": "@favi_ty/range",
  "version": "1.0.1",
  "author": "favi_ty <favius983@gmail.com>",
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.25.9",
    "@babel/preset-env": "7.25.9",
    "@babel/preset-modules": "0.1.2",
    "@babel/preset-typescript": "7.25.9",
    "@types/babel__core": "7.1.3",
    "@types/jest": "24.9.0",
    "@types/power-assert": "1.5.3",
    "babel-jest": "25.1.0",
    "babel-preset-power-assert": "3.0.0",
    "cross-env": "6.0.3",
    "jest": "25.1.0",
    "np": "5.2.1",
    "power-assert": "1.6.1",
    "terser": "4.6.3",
    "typescript": "3.7.5"
  },
  "license": "WTFPL",
  "main": "dist/range.js",
  "module": "dist/range.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist/ && yarn build:types && yarn build:module && yarn minify",
    "build:module": "cross-env NODE_ENV=production babel range.ts --out-dir dist --extensions '.ts'",
    "build:types": "tsc --emitDeclarationOnly",
    "minify": "terser dist/range.js --compress --source-map --mangle --output dist/range.js",
    "test": "jest --runInBand"
  },
  "types": "dist/types/range.d.ts"
}
